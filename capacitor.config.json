{
    "appId": "in.mapzo.app",
    "appName": "Mapzo",
    "webDir": "www",
    "server": {
        "androidScheme": "https",
        "hostname": "mapzo.in"
    },
    "plugins": {
        "SplashScreen": {
            "launchShowDuration": 1500,
            "backgroundColor": "#080808",
            "showSpinner": false,
            "splashFullScreen": true,
            "splashImmersive": true
        },
        "StatusBar": {
            "style": "DARK",
            "backgroundColor": "#0b0b0b"
        }
    }
}